package Week1;

public class Op1 {

	public static void main(String[] args) {
		int a=20, b=10, c=0;
		c=a+b;
		System.out.println(c);
		c=a-b;
		System.out.println(c);
		c=a*b;
		System.out.println(c);
		c=a/b;
		System.out.println(c);
		c=a%b;
		System.out.println(c);
		c=13%5;
		System.out.println(c);
		
	}

}
